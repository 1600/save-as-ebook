{

    "manifest_version": 2,
    "name": "Save As E-Book",
    "version": "1.0",

    "description": "Adds a solid red border to all webpages matching mozilla.org.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["fonts.css"],
        "js": ["jquery.js", "filesaver.js", "jszip.js", "jszip-utils.js", "sanitize-html.min.js", "pure-parser.js", "test.js"]
    }],

    "background": {
       "scripts": ["background.js"]
    },

    "permissions": [
        "contextMenus",
        "activeTab"
    ]

}
